{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [
            "error",
            {
                "code": 120
            }
        ]
    },
    "ignorePatterns": [
        "src/**/*.test.js",
        "src/fonts/*"
    ]
}